<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- DEFINICE POUZIVANYCH BAREV -->
    <Color x:Key="darkPurple">#FF550055</Color>
    <SolidColorBrush x:Key="darkPurpleBrush" Color="{StaticResource darkPurple}"/>

    <Color x:Key="lighterGray">#FFE5E5E5</Color>
    <SolidColorBrush x:Key="lighterGrayBrush" Color="{StaticResource lighterGray}"/>

    <Color x:Key="darkestGray">#FF333333</Color>
    <SolidColorBrush x:Key="darkestGrayBrush" Color="{StaticResource darkestGray}"/>

    <!--
    Arkitech
    Arkitech Round
    Arkitech Medium
    Baby Face
    Righteous
    Audiowide
    -->
    
    <FontFamily x:Key="mainFont">/resources/fonts/#Arkitech Medium</FontFamily>
    <!--FontFamily x:Key="mainFont">#Bauhaus 93</FontFamily-->
    
</ResourceDictionary>
